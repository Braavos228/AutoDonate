publics: OnPlayerCheckUnInviteLeader(playerid, name[])
{
	new rows, fields, tmp[20], mysql_query[156];
	cache_get_data(rows, fields);
	if(!rows)return SendMes(playerid, COLOR_GREY, "[ MySql ] Аккаунт %s не найден в базе данных.", name);
	cache_get_field_content(0, "pMRInections", tmp);
	if(strval(tmp) != PlayerInfo[playerid][pMRInections]) return SendMes(playerid,COLOR_GREY,"[ MySql ] Аккаунт %s не является вашим членом фракции.", name);

	format(mysql_query, sizeof(mysql_query), "UPDATE accounts SET pMRInections = '0', pRKInections = '0' WHERE Name = '%s'", name);
	mysql_function_query(connects, mysql_query, false, "", "");
	SendMes(playerid, COLOR_GREY, "[ MySql ] Аккаунт %s успешно уволен из фракции.", name);
	return 1;
}
