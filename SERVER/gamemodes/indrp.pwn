	}
	if(MehJob[playerid] ==2)
	{
		AvtoInfo[2][Rab] -=1;
		if( AvtoInfo[2][Rab] <0) AvtoInfo[2][Rab] = 0;
	}
	MehJob[playerid] = INVALID_PLAYER_ID;
	getdate(year, month, day);
	new hour, minute, second;
	gettime(hour, minute, second);
	GetPlayerIp(playerid,playersip,sizeof(playersip));
	if(IsPlayerAttachedObjectSlotUsed(playerid,1)) RemovePlayerAttachedObject(playerid,1);
	if(PlayerInfo[playerid][pJob] == 7) PlayerInfo[playerid][pJob] = 0;
	if(SelectChar[playerid] == INVALID_PLAYER_ID)
	{
		SetPlayerSkin(playerid, OldSkin[playerid]);
		PlayerInfo[playerid][pChar] = OldSkin[playerid];
		SelectChar[playerid] = 0;
	}
	tune[playerid] = 0;
	soglasen[playerid] = 0;
	if(mesto[playerid]==0) AvtoInfo[0][Mesto] = 255;
	if(mesto[playerid]==1) AvtoInfo[1][Mesto] = 255;
	if(mesto[playerid]==2) AvtoInfo[2][Mesto] = 255;
	SelectCharPlace[playerid] = 0;
	CharPrice[playerid] = 0;
	SelectCharID[playerid] = 0;
	SelectChar[playerid] = 0;
	PicCP[playerid] = 0;
	repairdvig[playerid] = 0;
	takephone[playerid] = 0;
	gag[playerid] = 0;
	dostup[playerid] = 0;
	PicCP[playerid] = 0;
	enterbiz[playerid] = 0;
	arenda[playerid] = 0;
	arenda[playerid] = 0;
	PlayerInfo[playerid][pDrugtime] = 0;
	PlayerInfo[playerid][pGunfbiTime] = 0;
	forma[playerid] = 0;

	KillTimer(STimer[playerid]);
	TextDrawDestroy(TextdrawFuel[playerid]);
	TextDrawDestroy(TextdrawSpeed[playerid]);
	TextDrawDestroy(TextdrawStatus[playerid]);
	TextDrawDestroy(TextdrawEngine[playerid]);

	proverkaforma[playerid] = 0;
	keys[playerid] = 0;
	SetPlayerSkills(playerid);
	SelectCharPlace[playerid] = 0;
	CharPrice[playerid] = 0;
	SelectCharID[playerid] = 0;
	SelectChar[playerid] = 0;
	if(TransportDuty[playerid] == 1) TaxiDrivers -= 1;
	if(PlayerInfo[playerid][pMRInections] == 4) Medics -= 1;
	if(PlayerInfo[playerid][pMRInections] == 1 || PlayerInfo[playerid][pMRInections] == 10 || PlayerInfo[playerid][pMRInections] == 21 ) Police +=1;
	else if(PlayerInfo[playerid][pJob] == 2) Mechanics -= 1;
	if(TransportCost[playerid] > 0 && TransportDriver[playerid] != INVALID_PLAYER_ID)
	{
		if(IsPlayerConnected(TransportDriver[playerid]))
		{
