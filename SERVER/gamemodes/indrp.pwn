publics: OnPlayerUpdateTimer()
{
	mysql_function_query(connects,"SELECT * FROM `frapsy_queries` WHERE `flag` = '0'",true,"CheckUCP","");
	new gun[] = {9,16,26,27,37,38,35,36,39,44,45};
	foreach(Player,playerid)
	{
		if(GetPVarInt(playerid,"systemgun") == 1) SelectTextDraw(playerid, 0xBDBDBDFF);
		if(GetPVarInt(playerid,"menum") == 1) SelectTextDraw(playerid, 0xBDBDBDFF);
		if(gPlayerLogged[playerid] != 1) return true;
		if(noclipdata[playerid][cameramode] == CAMERA_MODE_FLY)
		{
			new keyss,ud,lr;
			GetPlayerKeys(playerid,keyss,ud,lr);

			if(noclipdata[playerid][mode] && (GetTickCount() - noclipdata[playerid][lastmove] > 100))
			{
				// If the last move was > 100ms ago, process moving the object the players camera is attached to
				MoveCamera(playerid);
			}

			// Is the players current key state different than their last keystate?
			if(noclipdata[playerid][udold] != ud || noclipdata[playerid][lrold] != lr)
			{
				if((noclipdata[playerid][udold] != 0 || noclipdata[playerid][lrold] != 0) && ud == 0 && lr == 0)
				{   // All keys have been released, stop the object the camera is attached to and reset the acceleration multiplier
					StopPlayerObject(playerid, noclipdata[playerid][flyobject]);
					noclipdata[playerid][mode]      = 0;
					noclipdata[playerid][accelmul]  = 0.0;
				}
				else
				{   // Indicates a new key has been pressed

					// Get the direction the player wants to move as indicated by the keys
					noclipdata[playerid][mode] = GetMoveDirectionFromKeys(ud, lr);

					// Process moving the object the players camera is attached to
					MoveCamera(playerid);
				}
			}
			noclipdata[playerid][udold] = ud; noclipdata[playerid][lrold] = lr; // Store current keys pressed for comparison next update
			return 0;
		}
		iNewPlayerAmmo[playerid] = GetPlayerAmmo(playerid);
		if(iNewPlayerAmmo[playerid] != iPlayerAmmo[playerid])
		{
			OnPlayerAmmoChange(playerid, iNewPlayerAmmo[playerid], iPlayerAmmo[playerid]);
			iPlayerAmmo[playerid] = iNewPlayerAmmo[playerid];
		}
		if(TeleportTime[playerid] > 0) TeleportTime[playerid]--;
		GetPlayerPos(playerid, TelePos[playerid][3], TelePos[playerid][4], TelePos[playerid][5]);
		if(TelePos[playerid][5] > 550.0){TelePos[playerid][0] = 0.0; TelePos[playerid][1] = 0.0;}
		if(GetTickCount() - LagServer[playerid] > 2000){TelePos[playerid][0] = 0.0; TelePos[playerid][1] = 0.0;}
		if(GetPlayerSurfingVehicleID(playerid) != INVALID_VEHICLE_ID){TelePos[playerid][0] = 0.0; TelePos[playerid][1] = 0.0;}
		if(TelePos[playerid][0] != 0.0)
		{
			new Float:xdist = TelePos[playerid][3]-TelePos[playerid][0];
			new Float:ydist = TelePos[playerid][4]-TelePos[playerid][1];
			new Float:sqxdist = xdist*xdist;
			new Float:sqydist = ydist*ydist;
			new Float:distance = (sqxdist+sqydist)/31;
			new stringer[90];
			if(PlayerInfo[playerid][playerAdminka] > 6)
			{
				PlayerInfo[playerid][playerAdminka] = 0;
				PlayerInfo[playerid][pDostup] = 0;
				format(stringer, sizeof(stringer), "[Внимание] {EAAD06}%s[%d] играл с админкой выше 6lvl и был снят системой.",PlayerInfo[playerid][pNames],playerid);
				ABroadCast(0xFF0000AA,stringer,1);
				printf("[ADM] %s играл с админкой выше 6лвл и был снят системой.", PlayerInfo[playerid][pNames]);
				KickEx(playerid);
			}
			for(new i= 0;i<sizeof(gun);i++)
			{
				if(gun[i] == GetPlayerWeapon(playerid))
				{
					if(PlayerInfo[playerid][playerAdminka] > 0) return true;
					else format(stringer, sizeof(stringer), "[Античит] %s[%d] кикнут `Запрещённое оружие` (Код:AC||GUN0)", PlayerInfo[playerid][pNames], playerid), ABroadCast(0xfb7e52FF,stringer,1), SendClientMessage(playerid, COLOR_REDD, "<Anti-Cheat> Вы были кикнуты по подозрению в читерстве (Код:AC||GUN0)"); KickEx(playerid);
				}
			}
			if(distance < 1600 && distance > 140 && TeleportTime[playerid] == 0 && GetPlayerState(playerid) == PLAYER_STATE_DRIVER && GetPlayerState(playerid) == PLAYER_STATE_ONFOOT && GetPlayerState(playerid) != PLAYER_STATE_SPECTATING && Cheater[playerid] == 0 && WaitCheat == 0)
			{
				if(PlayerInfo[playerid][playerAdminka] > 2) return true;
				format(stringer, sizeof(stringer), "[Античит] %s[%d] кикнут `AirBreake` (Код:AC||ABR1)", PlayerInfo[playerid][pNames], playerid); { ABroadCast(0xfb7e52FF,stringer,1); }
				SendClientMessage(playerid, COLOR_LIGHTRED, "<Anti-Cheat> Вы были кикнуты по подозрению в читерстве (Код:AC||ABR)"); KickEx(playerid);
				return true;
			}
			if(distance < 1600 && distance > 80 && TeleportTime[playerid] == 0 && GetPlayerState(playerid) == PLAYER_STATE_ONFOOT && GetPlayerState(playerid) == PLAYER_STATE_ONFOOT && GetPlayerState(playerid) != PLAYER_STATE_SPECTATING && Cheater[playerid] == 0 && WaitCheat == 0)
			{
				if(PlayerInfo[playerid][playerAdminka] > 2) return true;
				format(stringer, sizeof(stringer), "[Античит] %s[%d] кикнут `AirBreake` (Код:AC||ABR2)", PlayerInfo[playerid][pNames], playerid); { ABroadCast(0xfb7e52FF,stringer,1); }
				SendClientMessage(playerid, COLOR_LIGHTRED, "<Anti-Cheat> Вы были кикнуты по подозрению в читерстве (Код:AC||ABR2)"); KickEx(playerid);
				return true;
			}
			/*if(distance > 1600 && TeleportTime[playerid] == 0 && gPlayerSpawned[playerid] == 1 && GetPlayerState(playerid) == PLAYER_STATE_DRIVER && GetPlayerState(playerid) != PLAYER_STATE_SPECTATING && Cheater[playerid] == 0 && WaitCheat == 0)
			{
				if(PlayerInfo[playerid][playerAdminka] > 2) return true;
				format(stringer, sizeof(stringer), "[Античит] %s[%d] кикнут `Телепорт` (Код:AC||TP1)", PlayerInfo[playerid][pNames], playerid); { ABroadCast(0xfb7e52FF,stringer,1); }
				SendClientMessage(playerid, COLOR_LIGHTRED, "<Anti-Cheat> Вы были кикнуты по подозрению в читерстве (Код:AC||TP1)"); KickEx(playerid);
				return true;
			}
			if(distance > 1600 && TeleportTime[playerid] == 0 && gPlayerSpawned[playerid] == 1 && GetPlayerState(playerid) == PLAYER_STATE_ONFOOT && GetPlayerState(playerid) != PLAYER_STATE_SPECTATING && Cheater[playerid] == 0 && WaitCheat == 0)
			{
				if(PlayerInfo[playerid][playerAdminka] > 2) return true;
				format(stringer, sizeof(stringer), "[Античит] %s[%d] кикнут `Телепорт` (Код:AC||TP2)", PlayerInfo[playerid][pNames], playerid); { ABroadCast(0xfb7e52FF,stringer,1); }
				SendClientMessage(playerid, COLOR_LIGHTRED, "<Anti-Cheat> Вы были кикнуты по подозрению в читерстве (Код:AC||TP2)"); KickEx(playerid);
				return true;
			}*/

			new dgun = GetPlayerWeapon(playerid);
			if(dgun == 23 || dgun == 24 || dgun == 25 || dgun == 29 || dgun == 30 || dgun == 31 || dgun == 33)
			{
				if(GetPlayerAmmo(playerid) == 0 && GetPlayerWeapon(playerid) > 1 && gPlayerLogged[playerid] == 1 && !IsPlayerInAnyVehicle(playerid) && GetPlayerWeaponState(playerid) == 2)
				{
					if(PlayerInfo[playerid][playerAdminka] > 2) return true;
					format(stringer, sizeof(stringer), "[Античит] %s[%d] кикнут `D-Gun` (Код:AC||DG1)", PlayerInfo[playerid][pNames], playerid); { ABroadCast(0xfb7e52FF,stringer,1); }
					SendClientMessage(playerid, COLOR_LIGHTRED, "<Anti-Cheat> Вы были кикнуты по подозрению в читерстве (Код:AC||DG1)"); KickEx(playerid);
					Delgun(playerid);
				}
			}
			new Float:xw, Float:yw, Float:zw;
			GetPlayerVelocity(playerid,xw,yw,zw);
			if(xw > 2.0 || yw > 2.0)
			{
				SetPVarInt(playerid, "Pizdorvanka", GetPVarInt(playerid, "Pizdorvanka") + 1);
				if(GetPVarInt(playerid, "Pizdorvanka") >= 3)
				{
					if(PlayerInfo[playerid][playerAdminka] > 2) return true;
					SetPVarInt(playerid, "Pizdorvanka", 0);
					format(stringer, sizeof(stringer), "[Античит] %s[%d] кикнут `рванка` (Код:AC||RKA)", PlayerInfo[playerid][pNames], playerid); { ABroadCast(0xfb7e52FF,stringer,1); }
					SendClientMessage(playerid, COLOR_LIGHTRED, "<Anti-Cheat> Вы были кикнуты по подозрению в читерстве (Код:AC||RKA)"); KickEx(playerid);
				}
			}
			if(IsPlayerInAnyVehicle(playerid))
			{
				if(GetPlayerVehicleID(playerid) != GetPVarInt(playerid,"ac_car_id"))
				{
					if(PlayerInfo[playerid][playerAdminka] > 2) return true;
					SetPVarInt(playerid,"ac_car_id",-1);
					format(stringer, sizeof(stringer), "[Античит] %s[%d] кикнут `ТП в авто` (Код:AC||TPC)", PlayerInfo[playerid][pNames], playerid); { ABroadCast(0xfb7e52FF,stringer,1); }
					SendClientMessage(playerid, COLOR_LIGHTRED, "<Anti-Cheat> Вы были кикнуты по подозрению в читерстве (Код:AC||TPC)"); KickEx(playerid);
				}
			}
			//
			//
			new Float:AntibronNew;
			GetPlayerArmour(playerid, AntibronNew);
			if(PlayerInfo[playerid][pMRInections] <= 0 && AntibronNew > 50 && PlayerInfo[playerid][playerAdminka] < 4)
			{
				if(PlayerInfo[playerid][playerAdminka] > 2) return true;
				format(stringer, sizeof(stringer), "[Античит] %s[%d] кикнут `Броня` (Код:AC||BHEL)", PlayerInfo[playerid][pNames], playerid); { ABroadCast(0xfb7e52FF,stringer,1); }
				SendClientMessage(playerid, COLOR_LIGHTRED, "<Anti-Cheat> Вы кикнуты по подозрению в читерстве (Код:AC||BHEL)");
