	}
	else
	{
		if(VEHHP[playerid][1] >= 9999.0)
		{
			format(stringer, sizeof(stringer), "%s[%d] был проверен на GM car. {FF0000}Ответ: GM car выявлено.", PlayerInfo[playerid][pNames],playerid);
			ABroadCast(COLOR_GREEN,stringer,2);
			SetVehicleHealth(GetPlayerVehicleID(playerid),VEHHP[playerid][0]);
