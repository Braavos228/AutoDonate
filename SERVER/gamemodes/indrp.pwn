publics: OnPlayerRegCheck(playerid)
{
	new rows, fields;
	SetPlayerFacingAngle(playerid, 186);
	InterpolateCameraPos(playerid, 1598.862792, -1437.580200, 366.292541, 1728.818359, -1700.671386, 93.946250, 11000);
	InterpolateCameraLookAt(playerid, 1595.242187, -1434.789550, 364.266876, 1725.602661, -1697.110351, 92.539710, 11000);
	for(new i = 0; i < 20; i++) SendClientMessage(playerid, COLOR_WHITE, "");
	cache_get_data(rows, fields);
	if(rows)
	{
		format(stringers, sizeof(stringers),"{009b69}Введите ваш действущий пароль.", PlayerInfo[playerid][pNames]);
		SPD(playerid,2,DIALOG_STYLE_PASSWORD,stringers,"{FFFFFF}Ваш логин был опознан в базе данных.\n\nУ вас есть: {5073B5}30 секунд{ffffff} для ввода пароля.\nПосле чего вы будете кикнуты сервером.","Ввод","Выход");
	}
	else
	{
		new palka = 0;
		if((PlayerInfo[playerid][pNames][0] > 'a' && PlayerInfo[playerid][pNames][0] < 'z') || strfind(PlayerInfo[playerid][pNames],"_",true,1) == -1)
		{
			SendClientMessage(playerid,COLOR_LIGHTRED,"Вы были кикнуты с сервера. Причина: NonRp ник. (Формат: Имя_Фамилия на английском)");
			KickEx(playerid);
			return true;
		}
		for (new i; i < strlen(PlayerInfo[playerid][pNames]); i++)
		{
			switch(PlayerInfo[playerid][pNames][i])
			{
			case '_':
				{
					palka++;
					if(palka > 1)
					{
						SendClientMessage(playerid,COLOR_LIGHTRED,"Вы были кикнуты с сервера. Причина: NonRp ник. (Формат: Имя_Фамилия на английском)");
						KickEx(playerid);
						break;
					}
					switch(PlayerInfo[playerid][pNames][i+1])
					{
					case 'a'..'z':
						{
							SendClientMessage(playerid,COLOR_LIGHTRED,"Вы были кикнуты с сервера. Причина: NonRp ник. (Формат: Имя_Фамилия на английском)");
							KickEx(playerid);
							break;
						}
					}
				}
			case '[',']','{','}','$','#','№','"','@','!',';','%','^',':','?','&','*','(',')','=','+','<','>':
				{
					SendClientMessage(playerid,COLOR_LIGHTRED,"Вы были кикнуты с сервера. Причина: NonRp ник. (Формат: Имя_Фамилия на английском)");
					KickEx(playerid);
					break;
				}
			case '0'..'9':
				{
					SendClientMessage(playerid,COLOR_LIGHTRED,"Вы были кикнуты с сервера. Причина: NonRp ник. (Формат: Имя_Фамилия на английском)");
					KickEx(playerid);
					break;
				}
			}
		}
		format(stringers, sizeof(stringers),"{009b69}Введите ваш будущий пароль от этого аккаунта:", PlayerInfo[playerid][pNames]);
		SPD(playerid,1,DIALOG_STYLE_PASSWORD,stringers,"{FFFFFF}Ваш логин не был опознан в базе данных.\nВведите в поле ниже ваш будущий пароль.\nПример сложного пароля может быть такой:\n - {009b69}kcOkzioC","Ввод","Выход");
	}
	return true;
}

