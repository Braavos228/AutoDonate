		PlayerInfo[playerid][pCash] += PlayerInfo[playerid][pHousecash];
		SendMes(playerid,COLOR_WHITE,"Вам возвращены %d вирт за ваш дом", PlayerInfo[playerid][pHousecash]);
		PlayerInfo[playerid][pHousecash] = 0;
	}
    return true;
}
