stock SetPlayerExtra(playerid,declare,reason[])
{
	if(IsPlayerConnected(playerid))
	{
		if(declare != 255) SendMes(playerid,COLOR_LIGHTRED,"Вы совершили преступление: [%s]. Сообщил: %s.",reason,PlayerInfo[declare][pNames]);
		else SendMes(playerid,COLOR_LIGHTRED,"Вы совершили преступление: %s. Сообщил: Неизвестно.",reason);
		if(PlayerInfo[playerid][pWanted] > 0)
		{
			new stringer[128];
			SendMes(playerid,COLOR_YELLOW,"Текущий уровень розыска: %d", PlayerInfo[playerid][pWanted]);
			SetPlayerWantedLevel(playerid, PlayerInfo[playerid][pWanted]);
			if(declare != 255) format(stringer,128,"Рация: Cообщает: %s. Преступление: %s. Подозреваемый: %s",PlayerInfo[playerid][pNames],reason,PlayerInfo[declare][pNames]);
			else format(stringer,128,"Рация: Cообщает: %s. Преступление: %s. Подозреваемый: Неизвестно",PlayerInfo[playerid][pNames],reason);
			SendFamilyMessage(1,COLOR_YELLOW2, stringer);
			SendFamilyMessage(2,COLOR_YELLOW2, stringer);
			SendFamilyMessage(3,COLOR_YELLOW2, stringer);
			SendFamilyMessage(10,COLOR_YELLOW2, stringer);
			SendFamilyMessage(21,COLOR_YELLOW2, stringer);
			SendFamilyMessage(25,COLOR_YELLOW2, stringer);
		}
	}
	return true;
}

publics: UpdateSpeedometr(playerid)
{
	if(IsPlayerInAnyVehicle(playerid))
	{
		new vehicleid; vehicleid = GetPlayerVehicleID(playerid);
		new locked[32],str[64],str2[64],str4[64],str5[64],Float:vehhel,vehh[20],vehs[20],vehe[20];
		if(IsLocked[GetPlayerVehicleID(playerid)] == 1) { locked = "~r~~h~Lock"; }
		else { locked = "~g~~h~Unlock";}
		GetVehicleHealth(GetPlayerVehicleID(playerid), vehhel);
		if(vehhel <= 500) vehh = "~r~";
		else vehh = "";
		if(Fuell[GetPlayerVehicleID(playerid)] <= 30) vehs = "~r~";
		else vehs = "";
		if(zavodis[playerid] == 1) vehe = "~g~ON";
		else vehe = "~r~OFF";
		format(str, sizeof(str),"%d KM/H",SpeedVehicle(playerid));
		format(str2, sizeof(str2), "%s", locked);
		format(str4, sizeof(str4), "%s%.0f", vehs,Fuell[GetPlayerVehicleID(playerid)]);
		format(str5, sizeof(str5), "%s", vehe);
		if(SpeedVehicle(playerid) !=0)
		{
			Fuell[vehicleid] -= 0.003;
			if(GetPlayerVehicleID(playerid) == caridhouse[playerid])
			{
				PlayerInfo[playerid][pFuelcar] -= 0.003;
			}
		}
		new carid = GetPlayerVehicleID(playerid);
		if(SuperGt(carid))
		{
			if(SpeedVehicle(playerid) !=0)
			{
				Fuell[vehicleid] -= 0.005;
				if(GetPlayerVehicleID(playerid) == caridhouse[playerid]) { PlayerInfo[playerid][pFuelcar] -=0.005; }
			}
		}
		if(Fuell[carid] < 0)
		{
			Fuell[carid] = 0;
			GetVehicleParamsEx(GetPlayerVehicleID(playerid),engine,lights,alarm,doors,bonnet,boot,objective);
			SetVehicleParamsEx(GetPlayerVehicleID(playerid),VEHICLE_PARAMS_OFF,VEHICLE_PARAMS_OFF,alarm,doors,bonnet,boot,objective); zavodis[playerid] = 0;
		}
		TextDrawSetString(TextdrawFuel[playerid],str4); TextDrawSetString(TextdrawSpeed[playerid],str); TextDrawSetString(TextdrawStatus[playerid],str2); TextDrawSetString(TextdrawEngine[playerid],str5);
	}
}
publics: OtherTimer()
{
	GetMoney();
	foreach(Player, i)
	{
		if(IsPlayerConnected(i))
		{
			if(gPlayerLogged[i] == 1)
			{
				if(GetPlayerState(i) == 2)
				{
					GetPlayerPos(i, TelePos[i][3], TelePos[i][4], TelePos[i][5]);
					if(TelePos[i][5] > 550.0) TelePos[i][0] = 0.0,TelePos[i][1] = 0.0;
				}
				if(TelePos[i][5] < 550.0 && TelePos[i][3] != 0.0) TelePos[i][0] = TelePos[i][3],TelePos[i][1] = TelePos[i][4];
			}
			if(CellTime[i] > 0)
			{
				if (CellTime[i] == cchargetime)
				{
					CellTime[i] = 1;
					if(Mobile[Mobile[i]] == i) CallCost[i] = CallCost[i]+callcost;
				}
				CellTime[i] = CellTime[i] +1;
				if (Mobile[Mobile[i]] == INVALID_PLAYER_ID && CellTime[i] == 5)
				{
					if(IsPlayerConnected(Mobile[i]))
					{
						new stringer[50];
						format(stringer, sizeof(stringer), "У %s звонит мобильник", PlayerInfo[Mobile[i]][pNames]);
						ProxDetector(30.0, Mobile[i], stringer, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);
					}
				}
			}
			if(CellTime[i] == 0 && CallCost[i] > 0)
			{
				new stringer[32];
				format(stringer, sizeof(stringer), "~w~Phone~n~~r~%d",CallCost[i]);
				PlayerInfo[i][pCash] -=-CallCost[i];
				GameTextForPlayer(i, stringer, 5000, 1);
				CallCost[i] = 0;
			}
		}
	}
	return true;
}

