stock PutPlayerInVeh(playerid, vehicleid, seatid)
{
	UseEnter[playerid] = false;
	IDVEH[playerid] = vehicleid;
	SetPVarInt(playerid,"ac_car_id",vehicleid);
	PutPlayerInVehicle(playerid, vehicleid, seatid);
	return true;
