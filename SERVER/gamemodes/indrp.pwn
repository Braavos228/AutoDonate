				KickEx(playerid);
			}
			//
			if(!IsPlayerInAnyVehicle(playerid)) if(!IsPlayerInAnyVehicle(playerid) && PlayerInfo[playerid][playerAdminka] < 4)
			{
				new Float:Z;
				GetPlayerPos(playerid,Z,Z,Z);
				new anim = GetPlayerAnimationIndex(playerid);
				if((anim == 1538 || anim == 1539 || anim == 1543) && Z > 2)
				{
					if(!NotFlyingHack(playerid))
					{
						if(PlayerInfo[playerid][playerAdminka] > 2) return true;
						format(stringer,sizeof(stringer),"[Античит] %s[%d] кикнут `Флай` (Код:AC||FLY)",PlayerInfo[playerid][pNames],playerid); ABroadCast(0xfb7e52FF,stringer,1);
						SendClientMessage(playerid, COLOR_LIGHTRED, "<Anti-Cheat> Вы были кикнуты по подозрению в читерстве (Код:AC||FLY)");
						Delgun(playerid);
						ResetPlayerWeapons(playerid);
