		}
		else
		{
			format(stringer, sizeof(stringer), "%s[%d] был проверен на GM. {FFFFFF}Ответ: GM не выявлено.", PlayerInfo[playerid][pNames],playerid);
			ABroadCast(COLOR_GREEN,stringer,2);
			SetPlayerHealthAC(playerid, GMHP[playerid][0]);
			SetPlayerArmourAC(playerid, GMARMOUR[playerid][0]);
			return true;
