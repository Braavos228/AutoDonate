		LagServer[playerid] = GetTickCount();
	}
	return true;
}
stock FixHour(hour)
{
	hour = timeshift+hour;
	if (hour < 0) { hour = hour+24; }
	else if (hour > 23) { hour = hour-24; }
	shifthour = hour;
	return true;
}
public OnPlayerKeyStateChange(playerid, newkeys, oldkeys)
{
	new stringer[300];
	if(newkeys == KEY_SECONDARY_ATTACK) return GangMenu(playerid);
	if((newkeys & KEY_FIRE))
	{
		if(IsAAntidm(playerid))
		{
			if(IsPlayerInAnyVehicle(playerid)) return true;
			if(PlayerInfo[playerid][playerAdminka] > 2) return true;
			SPD(playerid,1233,DIALOG_STYLE_MSGBOX,"{FFFF00}Анти-ДМ","{FFFFFF}Уважаемый игрок, данное место является зеленой зоной,\nт.е. на ней нельзя драться, вы были заморожены на пару секунд, приятной игры.","Закрыть","");
			AntiDM[playerid] = 5;
			TogglePlayerControllable(playerid, 0);
			return true;
		}
		return true;
	}
