		{
			if(IsACop(playerid)) GiveWeapon(playerid, 0, 1);
			if(PlayerInfo[playerid][pMRInections] == 5 || PlayerInfo[playerid][pLRInections] == 5) GiveWeapon(playerid, 5, 1);
			if(PlayerInfo[playerid][pMRInections] == 24 || PlayerInfo[playerid][pLRInections] == 24) GiveWeapon(playerid, 5, 1);
			if(PlayerInfo[playerid][pMRInections] == 6 || PlayerInfo[playerid][pLRInections] == 6) GiveWeapon(playerid, 8, 1);
			if(PlayerInfo[playerid][pMRInections] == 7 && PlayerInfo[playerid][pRKInections] == 3) GiveWeapon(playerid, 24, 100),SetPlayerArmourAC(playerid, 100);
			if(PlayerInfo[playerid][pMRInections] == 7 && PlayerInfo[playerid][pRKInections] == 4) GiveWeapon(playerid, 24, 100),SetPlayerArmourAC(playerid, 100);
			if(PlayerInfo[playerid][pMRInections] == 8 && PlayerInfo[playerid][pRKInections] == 2) GiveWeapon(playerid, 24, 100),SetPlayerArmourAC(playerid, 100);
			if(PlayerInfo[playerid][pMRInections] == 10 || PlayerInfo[playerid][pLRInections] == 10) GiveWeapon(playerid, 0, 1);
			if(PlayerInfo[playerid][pMRInections] == 11 || PlayerInfo[playerid][pLRInections] == 11) GiveWeapon(playerid, 0, 34);
