stock SellBizz(playerid)
{
	new str[64];
	if(PlayerInfo[playerid][pPbiskey] != 255)
	{
		new bouse = PlayerInfo[playerid][pPbiskey];
		BizzInfo[bouse][bLocked] = 1;
		BizzInfo[bouse][bOwned] = 0;
		BizzInfo[bouse][bProducts] = 0;
		strmid(BizzInfo[bouse][bOwner], "The State", 0, strlen("The State"), 255);
		strmid(BizzInfo[bouse][bExtortion], "No-one", 0, strlen("No-one"), 255);
		SendMes(playerid,COLOR_WHITE,"- Прибыль с продажи бизнеса: %d вирт", BizzInfo[bouse][bTill]);
		SendMes(playerid,COLOR_WHITE,"- Новый счёт в банке: %d вирт", PlayerInfo[playerid][pBank]);
		PlayerInfo[playerid][pBank] +=BizzInfo[bouse][bTill] / 2;
		PlayerInfo[playerid][pBank] +=BizzInfo[bouse][b2Till];
		BizzInfo[bouse][bTill] = 0;
		BizzInfo[bouse][b2Till] = 0;
		format(str, sizeof(str), "{ffffff}Цена: %d\nНа данный момент бизнес находится на продаже.",BizzInfo[bouse][bBuyPrice]);
		Update3DTextLabelText(BizzInfo[bouse][bLabel], COLOR_YELLOW, str);
		SaveBizz(bouse);
		PlayerInfo[playerid][pPbiskey] = 255;
	}
	return true;
}
