		case 13: VladGzone[1] +=1;
		case 15: VladGzone[2] +=1;
		case 17: VladGzone[3] +=1;
		case 18: VladGzone[4] +=1;
		}
	}
	// Менять тут
	FracBank[0][fGrove] += VladGzone[2]*200;
	FracBank[0][fVagos] += VladGzone[1]*200;
	FracBank[0][fRifa] += VladGzone[4]*200;
	FracBank[0][fAztek] += VladGzone[3]*200;
	FracBank[0][fBallas] += VladGzone[0]*200;
	SaveBankFrac();
	/////////
	foreach(Player, i)
	{
		if(PlayerInfo[i][pWanted] >= 1)
		{
			WantedLevel[i] -= 1;
			PlayerInfo[i][pWanted] -=1;
			SetPlayerWantedLevel(i, PlayerInfo[i][pWanted]);
		}
		new exp = PlayerInfo[i][pExp];
		new nxtlevel = PlayerInfo[i][pLevel]+1;
		new expamount = nxtlevel*levelexp;
		if(exp == expamount)
		{
			PlayerInfo[i][pLevel] += 1;
			PlayerInfo[i][pExp] = 0;
			PlayerInfo[i][pAge] += 1;
			SendClientMessage(i,COLOR_WHITE,"Поздравляем! Ваш IC возраст был повышен. Удачной игры.");
			SetPlayerScore(i, PlayerInfo[i][pLevel]);
		}
		if(armmatbi < 0) armmatbi = 0;
		else if(armmatsf < 0) armmatsf = 0;
		else if(kazna < 0) kazna = 0;
		else if(lspdmatbi < 0) lspdmatbi = 1000;
		else if(sfpdmats < 0) sfpdmats = 1000;
		else if(lvpdmats < 0) lvpdmats = 1000;
		else if(fbimats < 0) fbimats = 1000;
		else if(yakuzamats < 0) yakuzamats = 1000;
		else if(rmmats < 0) rmmats = 1000;
		else if(lcnmats < 0) lcnmats = 1000;
		else if(rifamatbi < 0) rifamatbi = 1000;
		else if(ballasmatbi < 0) ballasmatbi = 1000;
		else if(groovmatbi < 0) groovmatbi = 1000;
		else if(vagosmatbi < 0) vagosmatbi = 1000;
		else if(aztekmatbi < 0) aztekmatbi = 1000;
		new moneys = 1+random(400);
		switch(PlayerInfo[i][pMRInections])
		{
		case 1,10,21,25:{ PlayerInfo[i][pPayCheck] += 300*PlayerInfo[i][pRKInections]+moneys; } // ЗП полиции, 2к умноженое на ранг + рандомные цифры
		case 3,19:{ PlayerInfo[i][pPayCheck] += 300*PlayerInfo[i][pRKInections]+moneys; } // Зп армии, 1.5к умноженое на ранг + рандомные цифры
		case 2:{ PlayerInfo[i][pPayCheck] += 500*PlayerInfo[i][pRKInections]+moneys; } // Зп ФБР, 3к умноженое на ранг + рандомные цифры
		case 7:{ PlayerInfo[i][pPayCheck] += 300*PlayerInfo[i][pRKInections]+moneys; } // Зп Мэрия, 4к умноженое на ранг + рандомные цифры
		case 4,22:{ PlayerInfo[i][pPayCheck] += 300*PlayerInfo[i][pRKInections]+moneys; } // Зп МинЗдрав, 4к умноженое на ранг + рандомные цифры
		case 11:{ PlayerInfo[i][pPayCheck] += 300*PlayerInfo[i][pRKInections]+moneys; } // Зп инструкторов, 4к умноженое на ранг + рандомные цифры
		}
		PlayerInfo[i][pExp]++;
		new checks = PlayerInfo[i][pPayCheck];
		GameTextForPlayer(i, "~g~PayDay", 5000, 1);
		apayday[i]++;
		//-------------------------- Работам -------------------
		SendClientMessage(i, COLOR_WHITE, "________[ Клиент Банка SA ]_________");
		SendMes(i,COLOR_WHITE,"Гос. налог: {cf800b}-%d$", nalog);
		SendMes(i,COLOR_WHITE,"Услуги телефонии: {cf800b}-%d$",PlayerInfo[i][pMobile]);
