		PlayerInfo[i][pBank] += checks;
		SendMes(i,COLOR_WHITE,"Ваш баланс: {2dc42d}%d$", PlayerInfo[i][pBank]);
		PlayerInfo[i][pZakonp] += 1;
		PlayerInfo[i][pBank] += PlayerInfo[i][pMobile];
		PlayerInfo[i][pMobile] = 0;
		PlayerInfo[i][pBank] -= nalog;
		kazna +=nalog;
		SaveMeria();
		new bouse = PlayerInfo[i][pPbiskey];
		if(PlayerInfo[i][pPbiskey] != 255)
		{
			SendClientMessage(i, COLOR_WHITE, "Электричество бизнеса: {cf800b}-300$");
			if(bouse != 255)
			{
				BizzInfo[bouse][bTill] -= 300;
				BizzInfo[bouse][bTill] += BizzInfo[bouse][b2Till];
				BizzInfo[bouse][b2Till] = 0;
				if(BizzInfo[bouse][bMafia] !=0)
				{
					if(BizzInfo[bouse][bTill] > 5000)
					{
						BizzInfo[bouse][bTill] -= 1000;
						if(BizzInfo[bouse][bMafia] == 6) MafiaBank[0][nYakuza] +=500;
						if(BizzInfo[bouse][bMafia] == 14) MafiaBank[0][nRm] +=500;
						if(BizzInfo[bouse][bMafia] == 5) MafiaBank[0][nLcn] +=500;
					}
				}
				SaveBizz(bouse);
			}
		}
		SendClientMessage(i,COLOR_WHITE,"___________________________________");
		if(PlayerInfo[i][punWarns] == 1) { PlayerInfo[i][punWarnstime] += 1; SendMes(i,COLOR_WHITE,"Автоматическое снятие варна через %d часов", 10 - PlayerInfo[i][punWarnstime]); }
